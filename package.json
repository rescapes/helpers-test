{
  "name": "@rescapes/helpers-test",
  "version": "0.0.225",
  "description": "Helpers for testing components",
  "main": "src/index.mjs",
  "module": "src/index.mjs",
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescapes/helpers-test"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/rescapes/helpers-test"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "rollup -c",
    "prepare": "cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run build",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@chakra-ui/react": "^1.7.4",
    "core-js": "^3.20.3",
    "decamelize": "^6.0.0",
    "folktale": "^2.3.2",
    "framer-motion": "^5.6.0",
    "graphql-tools": "^8.2.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-adopt": "^0.6.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "regenerator-runtime": "^0.13.9",
    "render-props": "^1.1.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@rescapes/apollo": ">=0.1.627",
    "@rescapes/helpers": ">=1.1.129",
    "@rescapes/helpers-component": ">=1.5.163",
    "@rescapes/log": ">=0.1.11",
    "@rescapes/place": ">=0.1.151",
    "@rescapes/ramda": ">=1.6.196",
    "@rescapes/validate": ">=1.2.23",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "14.2.0",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.6.2",
    "enzyme-wait": "^1.0.9",
    "eslint": "^8.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^25.7.0",
    "graphql": ">=16.2.0",
    "install": "^0.13.0",
    "jest": "27.4.7",
    "jest-cli": "^27.4.7",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "localstorage-polyfill": "^1.0.1",
    "node-fetch": "^3.1.1",
    "ramda": ">=0.28.0",
    "react-dev-utils": "^12.0.0",
    "react-router-dom": "^6.2.1",
    "rollup": "^2.64.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-watch": "4.3.1"
  },
  "peerDependencies": {
    "@rescapes/apollo": ">=0.0.74",
    "@rescapes/helpers": ">=1.1.109",
    "@rescapes/helpers-component": ">=1.5.64",
    "@rescapes/log": ">=0.1.11",
    "@rescapes/place": ">=0.0.337",
    "@rescapes/ramda": ">=1.5.78",
    "@rescapes/validate": ">=1.2.23"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme",
      "<rootDir>/config/jestsetup.js"
    ],
    "testEnvironment": "jest-environment-node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testURL": "http://localhost",
    "transform": {},
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
