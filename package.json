{
  "name": "@rescapes/helpers-test",
  "version": "0.0.163",
  "description": "Helpers for testing components",
  "main": "src/index.mjs",
  "module": "src/index.mjs",
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rescapes/helpers-test"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/rescapes/helpers-test"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "rollup -c",
    "prepare": "cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run build",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@chakra-ui/react": "^1.5.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11.3.0",
    "apollo-test-utils": "^0.3.2",
    "core-js": "^3.10.1",
    "decamelize": "^5.0.0",
    "folktale": "^2.3.2",
    "framer-motion": "^4",
    "graphql-tools": "^7.0.4",
    "longjohn": "^0.2.12",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-adopt": "^0.6.0",
    "react-apollo": "3.1.5",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.13",
    "react-redux": "^7.2.3",
    "rebass": "^4.0.2",
    "redux": "^4.0.4",
    "regenerator-runtime": "^0.13.7",
    "render-props": "^1.1.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@apollo/react-testing": "^4.0.0",
    "@rescapes/apollo": ">=0.1.508",
    "@rescapes/helpers": ">=1.1.115",
    "@rescapes/helpers-component": ">=1.5.145",
    "@rescapes/log": ">=0.1.11",
    "@rescapes/place": ">=0.0.411",
    "@rescapes/ramda": ">=1.6.172",
    "@rescapes/validate": ">=1.2.23",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.6.2",
    "enzyme-wait": "^1.0.9",
    "eslint": "^7.24.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.3.5",
    "graphql": ">=15.5.0",
    "install": "^0.13.0",
    "jest": "26.6.3",
    "jest-cli": "^26.6.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.0",
    "jsdom": "^16.5.3",
    "jsdom-global": "^3.0.2",
    "localstorage-polyfill": "^1.0.1",
    "ramda": ">=0.27.0",
    "react-dev-utils": "^11.0.4",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.45.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-watch": "4.3.1"
  },
  "peerDependencies": {
    "@rescapes/apollo": ">=0.0.74",
    "@rescapes/helpers": ">=1.1.109",
    "@rescapes/helpers-component": ">=1.5.64",
    "@rescapes/log": ">=0.1.11",
    "@rescapes/place": ">=0.0.337",
    "@rescapes/ramda": ">=1.5.78",
    "@rescapes/validate": ">=1.2.23"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme",
      "<rootDir>/config/jestsetup.js"
    ],
    "testEnvironment": "jest-environment-node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testURL": "http://localhost",
    "transform": {},
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
